{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/crack",
      "destination": "/index.html"
    },
    {
      "source": "/mojon",
      "destination": "/public/html/mojon.html"
    }
  ],
  "builds": [
    { "src": "/api/index.js", "use": "@vercel/node" },
    { "src": "/index.html", "use": "@vercel/static" },
    { "src": "/public/html/mojon.html", "use": "@vercel/static" }
  ]
}

